<form method="post" action="{{ path('categorie_delete', {'catId': categorie.catId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.catId) }}">
    <button class="btn">Delete</button>
</form>
