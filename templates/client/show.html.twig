{% extends 'admin/index.html.twig' %}

{% block title %}Client{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
    <h1>Client</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdUtilis</th>
                <td>{{ client.idUtilis }}</td>
            </tr>
            <tr>
                <th>UtilNom</th>
                <td>{{ client.utilNom }}</td>
            </tr>
            <tr>
                <th>UtilPrenom</th>
                <td>{{ client.utilPrenom }}</td>
            </tr>
            <tr>
                <th>UtilSexe</th>
                <td>{{ client.utilSexe }}</td>
            </tr>
            <tr>
                <th>UtilAdresse</th>
                <td>{{ client.utilAdresse }}</td>
            </tr>
            <tr>
                <th>UtilCp</th>
                <td>{{ client.utilCp }}</td>
            </tr>
            <tr>
                <th>UtilVille</th>
                <td>{{ client.utilVille }}</td>
            </tr>
            <tr>
                <th>UtilMail</th>
                <td>{{ client.utilMail }}</td>
            </tr>
            <tr>
                <th>UtilUsername</th>
                <td>{{ client.utilUsername }}</td>
            </tr>
            <tr>
                <th>UtilPassword</th>
                <td>{{ client.utilPassword }}</td>
            </tr>
            <tr>
                <th>UtilTelephone</th>
                <td>{{ client.utilTelephone }}</td>
            </tr>
            <tr>
                <th>UtilDateDeNaissance</th>
                <td>{{ client.utilDateDeNaissance ? client.utilDateDeNaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>CliId</th>
                <td>{{ client.cliId }}</td>
            </tr>
            <tr>
                <th>CliAdresseLivraison</th>
                <td>{{ client.cliAdresseLivraison }}</td>
            </tr>
            <tr>
                <th>CliType</th>
                <td>{{ client.cliType ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CliCoefficient</th>
                <td>{{ client.cliCoefficient }}</td>
            </tr>
            <tr>
                <th>CliReference</th>
                <td>{{ client.cliReference }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('client_index') }}">back to list</a>

    <a href="{{ path('client_edit', {'idUtilis': client.idUtilis}) }}">edit</a>

    {{ include('client/_delete_form.html.twig') }}
       </div>
</div>
{% endblock %}
